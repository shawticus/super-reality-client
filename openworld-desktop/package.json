{
  "name": "openworld-desktop",
  "version": "1.0.0",
  "description": "",
  "main": "lib/main.js",
  "scripts": {
    "build": "run-s clean parcel:build electron:build",
    "clean": "run-p parcel:clean electron:clean",
    "dist": "run-s clean parcel:build electron:dist",
    "electron:build": "electron-builder --dir",
    "electron:clean": "rimraf dist",
    "electron:dist": "electron-builder",
    "electron:start": "electron .",
    "install:wsl": "npm install && npm uninstall electron && export npm_config_platform=win32 && npm install --save-dev electron && unset npm_config_platform",
    "parcel:build": "parcel build src/openworld/index.html --target electron --out-dir build --public-url ./",
    "parcel:clean": "rimraf build",
    "parcel:start": "parcel src/openworld/index.html --no-hmr --target electron --out-dir build --public-url ./",
    "start": "run-p parcel:start electron:start",
    "test": "eslint src",
    "webpack:clean": "rimraf lib",
    "webpack:all": "webpack --config webpack/webpack.dev.js && npm run electron:start",
    "webpack:main": "webpack --config webpack/webpack.dev.main.js && npm run electron:start",
    "webpack:renderer": "webpack --config webpack/webpack.dev.renderer.js && npm run electron:start"
  },
